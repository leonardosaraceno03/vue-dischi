                                  }

                                  if(typeof currentExpObj['callback'] == "function" && variant == currentExpObj['variant']){
                                    currentExpObj['callback']();
                                  }else if(currentExpObj['disableLoaderBeforeResponse']){
                                      currentInstance.startLongLoader(currentExpObj.isMobile);
                                      if(typeof currentExpObj.defaultCallback == "function") {
                                          currentExpObj.defaultCallback();
                                      }
                                 }
                                  classObj.createCookie("runtest",runTestCookie,0, currentInstance.getDomainName());
                               }
                              window.RDL[currentExpObj.rdlKey]=variant;
                              window.RDL.disableHIW=false;
                         }else{
                             window.RDL.disableHIW=false;
                            currentInstance.startLongLoader(currentExpObj.isMobile);
                            if(typeof currentExpObj.defaultCallback == "function") {
                                currentExpObj.defaultCallback();
                            }
                         }
                   }
                }
       
   }

      visitorExperimentHelper.prototype.merge_options=function(obj1,obj2){
      var obj3 = {};
      for (var attrname in obj1) { obj3[attrname] = obj1[attrname]; }
      for (var attrname in obj2) { obj3[attrname] = obj2[attrname]; }
      return obj3;
   }
            
      visitorExperimentHelper.prototype.GetQueryString=function(field) {
            var href = window.location.href;
            var reg = new RegExp('[?&]' + field + '=([^&#]*)', 'i');
            var string = reg.exec(href);
            return string ? string[1] : null;
      }
     
   visitorExperimentHelper.prototype.getExperimentVariantByQueryParams=function(experimentObj){
       var restProcess=false;
       var currentInstance=this;
         if(currentInstance.GetQueryString(experimentObj['queryParam']) == "true"){
               RDL[experimentObj.rdlKey]=experimentObj['variant'];
               if(typeof experimentObj.callback == "function"){
                   experimentObj.callback();
               }
               var runTestCookie=currentInstance.getCookie("runtest");
               if(runTestCookie && runTestCookie != ""){
                  runTestCookie=runTestCookie + "," + experimentObj['id'] + "_" + experimentObj['variant'];
                }else{
                     runTestCookie=experimentObj['id'] + "_" + experimentObj['variant'];
               }
                var expVariant=currentInstance.getExperimentVariantFromCookie(experimentObj['id']);
               if(!expVariant)
               classObj.createCookie("runtest",runTestCookie,0, currentInstance.getDomainName());
                    
          }else{
            restProcess=true;
          }
     return restProcess;
 }  

   
     visitorExperimentHelper.prototype.conductVisitorExperiment=function(currentExpObj,visitorID){

       var variant=0;
       var xhttp=new XMLHttpRequest();
       var ebBaseUrl="https://www.livecareer.it/eb/";
       var currentInstance=this;

       xhttp.open("POST",ebBaseUrl+'api/v1/visitors/' + visitorID+ '/experiments/' +currentExpObj['id']+"/conduct")
       xhttp.setRequestHeader('Content-type', 'application/json')
       xhttp.send(JSON.stringify({
        visitor_id:visitorID,
        experiment_id:currentExpObj['id']
       }));

       xhttp.onerror=function(){     
            window.RDL.disableHIW = false;
           if (typeof currentExpObj.defaultCallback == "function") {
               currentExpObj.defaultCallback();
           }
           currentInstance.startLongLoader(currentExpObj.isMobile);
       }
       xhttp.onload = function() {

            if(xhttp.responseText){
                var response=JSON.parse(xhttp.responseText);
                if(response.data){
                    variant=response.data.variant;
                    if(typeof classObj.createCookie == "function" && typeof currentInstance.getCookie == "function"){
                                   var runTestCookie=currentInstance.getCookie("runtest");
                                   if(runTestCookie && runTestCookie != ""){
                                     runTestCookie=runTestCookie + "," + currentExpObj['id'] + "_" + variant;
                                  }else{
                                       runTestCookie=currentExpObj['id'] + "_" + variant;
                                  }
                                 classObj.createCookie("runtest",runTestCookie,0, currentInstance.getDomainName());
                               }
                              window.RDL[currentExpObj.rdlKey]=variant;
                              if(typeof currentExpObj.callback == "function" && variant == currentExpObj['variant']){
                                    currentExpObj.callback();
                              }else if(currentExpObj['disableLoaderBeforeResponse']){
                                  currentInstance.startLongLoader(currentExpObj.isMobile);
                                  if (typeof currentExpObj.defaultCallback == "function") {
                                      currentExpObj.defaultCallback();
                                  }
                              }
                              window.RDL.disableHIW=false;

                }else{
                         window.RDL.disableHIW=false; 
                         currentInstance.startLongLoader(currentExpObj.isMobile);   
                         if (typeof currentExpObj.defaultCallback == "function") {
                            currentExpObj.defaultCallback();
                         }
                }
               
            }

       }

 }

  
	  visitorExperimentHelper.prototype.init =function (experimentObj,isLiteBuilder){

          if ((this.getCookie("disabletests") == 1) && (this.getCookie("runtest") != experimentObj.id + "_" + experimentObj.variant)) {
              if (typeof experimentObj.defaultCallback == "function") {
                  experimentObj.defaultCallback();
              }
            return 0;
          }


          var interval;
          var currentInstance=this;
          var restExperimentObj=this.getExperimentVariantByQueryParams(experimentObj);
          window.RDL.disableHIW=false;
          if(experimentObj['disableLoaderBeforeResponse']){
                  window.RDL.disableHIW=true;
                  currentInstance.closeLongLoader(experimentObj.isMobile);
                }else{
                  currentInstance.startLongLoader(experimentObj.isMobile);
             }

          var expVariant=currentInstance.getExperimentVariantFromCookie(experimentObj['id']);

          if(expVariant){
            RDL[experimentObj.rdlKey]=expVariant;
            if(typeof experimentObj.callback == "function" && experimentObj.variant == expVariant){
                experimentObj.callback();
                window.RDL.disableHIW=false;
            }else if(experimentObj['disableLoaderBeforeResponse']){
                window.RDL.disableHIW = false;
                if (typeof experimentObj.defaultCallback == "function") {
                    experimentObj.defaultCallback();
                }
                currentInstance.startLongLoader(experimentObj.isMobile); 
            }

             return 0;                  
          }

          

          if(!interval && restExperimentObj){
            return new Promise(function(resolve,reject){
                var timer=0;
                var visitorID=currentInstance.getCookie("vstr");
                if(isLiteBuilder){
                 visitorID=currentInstance.getCookie("spvstr");
                }
                if(!visitorID){
                    timer=50;   
                }
             interval=setInterval(function(){
                if(typeof currentInstance.getCookie != "function" || typeof currentInstance.GetQueryString != "function"  ||      
                typeof currentInstance.getVisitorVariant !="function"){
                    return '';
                 }
                visitorID=currentInstance.getCookie("vstr");
                if(isLiteBuilder){
                 visitorID=currentInstance.getCookie("spvstr");
                }
                 if(visitorID){
                        clearInterval(interval); 
                        if (!expVariant) {
                            var isConductExperiment = true;
                            var expUserStatus = currentInstance.getCookie("UserStatus");
                            if(expUserStatus) {
                                expUserStatus = JSON.parse(expUserStatus);
                                isConductExperiment = expUserStatus && expUserStatus.User ? (expUserStatus.User.Role == 0?true:false) : true;
                            }
                            window.RDL.disableHIW=true;
                            if(!experimentObj['allowConduction']){
                                currentInstance.getVisitorVariant(experimentObj['id'],visitorID,experimentObj);
                            } else {
                                if (isConductExperiment) {
                                    currentInstance.conductVisitorExperiment(experimentObj, visitorID);
                                } else {
                                    currentInstance.getVisitorVariant(experimentObj['id'], visitorID, experimentObj);
                                }
                            }  
                        }
                        resolve("");
                    }

             },timer);
           
           
            })
          }
            
          
   }


     window.RDL.visitorExperimentHelper = new visitorExperimentHelper();
        
