                    window.location.href = window.location.href + "&disabletestcookie=added";
                }
            }
        }

        readDisableTest.prototype.getQSOnLoad = function (field) {
            var href = window.location.href;
            var qstring = "";
            if (href.indexOf(field) >= 0) {
                var reg = new RegExp('[?&]' + field + '=([^&#]*)', 'i');
                var string = reg.exec(href);
                var rtColl = string[1].split(",");
                for (var i = 0; i < rtColl.length; i++) {
                    var currElem = rtColl[i];
                    if (currElem) {
                        qstring = i > 0 ? (qstring + ("," + currElem.substr(0, currElem.indexOf("_") + 2))) : currElem.replace(",", "");
                    }
                }
            }
            return qstring ? qstring : null;
        };

        readDisableTest.prototype.createCookie = function (name, value, days, domain) {
            var expires = "";
            var _domain = "";
            if (days) {
                var date = new Date();
                date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
                expires = "; expires=" + date.toGMTString();
            } else
                expires = "";

            if (domain) {
                _domain = "; domain=" + domain;
            }
            document.cookie = name + "=" + value + expires + _domain + "; path=/;";
