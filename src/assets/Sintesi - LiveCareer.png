    </script>
    <style>
        /*******Animation css : Start*********/
        /*** Long Loading Animation **/
        .preloader-overlay {
            background: #fff;
            position: fixed;
            width: 100%;
            height: 100%;
            z-index: 99999;
            top: 0;
        }

        .preloader-wrapper {
            position: fixed;
            width: auto;
            left: 50%;
            top: 50%;
            margin: -180px 0 0 -133px;
        }

        .animation-fold {
            margin: 20px auto 0;
            overflow: hidden;
            position: relative;
            height: 105px;
        }

        .loading {
            font-family: 'Source Sans Pro',sans-serif;
            font-size: 15px;
            font-weight: 400;
            color: #000;
            position: relative;
            -webkit-animation: Loader 7s forwards;
            animation: Loader 7s 16s forwards;
            animation-delay: 1s;
            letter-spacing: .4px;
        }

            .loading .seven, .loading .six {
                animation-duration: 4s;
                animation-fill-mode: forwards;
            }

            .loading > div {
                margin-bottom: 12px;
                font-family: 'Source Sans Pro';
                letter-spacing: .4px;
            }

            .loading div:first-child {
                margin-top: 35px;
            }

            .loading > div span {
                font-size: 12px;
                padding: 5px 4px;
                font-style: normal;
                margin-right: 15px;
                background-color: #fff;
                color: #fd9031;
                background-repeat: no-repeat;
                background-position: 50% 48%;
                border: 1px solid #fd9031;
                border-radius: 20px;
                width: 23px;
                height: 23px;
                vertical-align: middle;
            }

        .gradient, .top-content-hide {
            position: absolute;
            width: 100%;
        }

        .loading .first {
            animation-name: firstDiv;
            animation-duration: 4s;
            animation-delay: 0s;
        }

        .loading .second {
            animation-name: secondDiv;
            animation-duration: 4s;
            animation-delay: 1s;
        }

        .loading .third {
            animation-name: thirdDiv;
            animation-duration: 4s;
            animation-delay: 2s;
        }

        .loading .four {
            animation-name: fourDiv;
            animation-duration: 4s;
            animation-delay: 3s;
        }

        .loading .five {
            animation-name: fiveDiv;
            animation-duration: 4s;
            animation-delay: 4s;
        }

        .loading .six {
            animation-name: sixDiv;
            animation-delay: 5s;
        }

        .loading .seven {
            animation-name: sevenDiv;
            animation-delay: 6s;
        }

        .top-content-hide {
            background: #3d4c61;
            top: 0;
            z-index: 5;
            height: 15px;
        }

        .gradient {
            height: 35px;
            left: 0;
            bottom: 0;
        }

        .grad-top {
            top: 0;
            height: 50px;
            z-index: 1;
            background: -moz-linear-gradient(top,rgba(255,255,255,1) 0,rgba(255,255,255,0) 100%);
            background: -webkit-linear-gradient(top,rgba(255,255,255,1) 0,rgba(255,255,255,0) 100%);
            background: linear-gradient(to bottom,rgba(255,255,255,1) 0,rgba(255,255,255,0) 100%);
            filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#00ffffff', GradientType=0);
        }

        .grad-bottom {
            background: -moz-linear-gradient(top,rgba(255,255,255,0) 0,rgba(255,255,255,1) 100%);
            background: -webkit-linear-gradient(top,rgba(255,255,255,0) 0,rgba(255,255,255,1) 100%);
            background: linear-gradient(to bottom,rgba(255,255,255,0) 0,rgba(255,255,255,1) 100%);
            filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#00ffffff', endColorstr='#ffffff', GradientType=0);
        }

        @keyframes Loader {
            0% {
                top: 0;
            }

            14.2% {
                top: -35.6px;
            }

            28.4% {
                top: -71.3px;
            }

            42.6% {
                top: -107px;
            }

            56.6% {
                top: -142px;
            }

            71% {
                top: -178px;
            }

            100% {
                top: -180.6px;
            }
        }

        @keyframes firstDiv {
            0% {
                background-color: #fd9031;
                color: #fff;
                background-repeat: no-repeat;
                background-position: 50% 48%;
            }
        }

        @keyframes secondDiv {
            14.2% {
                background-color: #fd9031;
                color: #fff;
            }
        }

        @keyframes thirdDiv {
            28.4% {
                background-color: #fd9031;
                color: #fff;
            }
        }

        @keyframes fourDiv {
            42.6% {
                background-color: #fd9031;
